var fiba
var fibb
var temp
function myfib2(thresh) {
  fiba = 1;
  fibb = 1;
  document.write (fiba + ", " + fibb + ", ");
  while(fibb <= thresh) {
    temp = fiba + fibb;
    fiba = fibb;
    fibb = temp;
    if (fibb > thresh) break;
    document.write (fibb + ", ");
  }
  document.write("Break now...\n");
}
myfib2(150)
